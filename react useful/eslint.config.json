{
    "ecmaFeatures": {
        "jsx": true
    },

    "env": {
        "browser": true,
        "node": true,
        "jquery": true,
        "es6": true
    },
    "globals": {
        "document": false,
        "navigator": false,
        "window": false,
        "jQuery": true,
        "$": true

    },

    "rules": {
        "no-lone-blocks": 0,
        "eol-last": 0,
        "no-alert": 0,
        "no-trailing-spaces": 0,
        "no-unsaved-vars": 0,
        "quotes": 0,
        "no-underscore-dangle": 0
    }

}